{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@api": [
        "./src/api/*"
      ],
      "@assets": [
        "./src/assets/*"
      ],
      "@components": [
        "./src/components/*"
      ],
      "@context": [
        "./src/context/*"
      ],
      "@pages": [
        "./src/pages/*"
      ],
      "@routes": [
        "./src/routes/*"
      ],
      "@services": [
        "./src/services/*"
      ],
      "@types": [
        "./src/types/*"
      ],
      "@utils": [
        "./src/utils/*"
      ]
    },
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
